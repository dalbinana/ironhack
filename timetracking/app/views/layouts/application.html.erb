<!DOCTYPE html>
<html>
<head>
  <title>Timetracking</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' 'jquery-rails'=> true %>
  <%= csrf_meta_tags %>
  <script>
  $(window).load(function(){
  $("#message").fadeOut(2000);
});
  </script>
</head>
<body>
<%= render 'shared/header' %> 
<%= flash_message %>
<%= yield %>

</body>
</html>
